on exitFrame
    put "base item delimiter", the itemDelimiter
    set the itemDelimiter = ":"
    put "a:b:c" into field 2
    put "num of items:", the number of items of field 2
    set the itemDelimiter = ","
    put "again:", the number of items of field 2
    put "hey" before item 1 of field 2
    put field 2
    put "you" after item 1 of field 2
    put field 2
    put "me" into item 2 of field 2
    put field 2
    delete item 1 of field 2
    put field 2
end
